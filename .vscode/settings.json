{
    "openhab.useRestApi": false,
    "java.enabled": false,
    "redhat.telemetry.enabled": false,
    "files.exclude": {
        "**/*.class": true,
        "**/*.jar": true
    },
    "files.watcherExclude": {
        "**/openhab-addons/bundles/org.openhab.binding.!(net2)*/**": true,
        "**/openhab-addons/itests/**": true,
        "**/*.items": true,
        "**/*.rules": true,
        "**/*.things": true,
        "**/*.sitemap": true,
        "**/*.persist": true
    },
    "search.exclude": {
        "**/openhab-addons/bundles/org.openhab.binding.!(net2)*/**": true
    },
    "java.import.exclusions": [
        "**/openhab-addons/bundles/org.openhab.automation.*/**",
        "**/openhab-addons/bundles/org.openhab.binding.!(net2)*/**",
        "**/openhab-addons/itests/**"
    ],
    "java.configuration.updateBuildConfiguration": "disabled",
    "java.autobuild.enabled": false
}